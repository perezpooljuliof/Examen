<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">

    <application>
        <el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
  	</application>
  	
  	<managed-bean>
		<managed-bean-name>empresaBean</managed-bean-name>
		<managed-bean-class>mx.com.examen.view.EmpresaManagedBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>empresaServicio</property-name>
			<value>#{empresaServicio}</value>
		</managed-property>
		<!--
		<managed-property>
			<property-name>nombre</property-name>
			<value>#{nombre}</value>
		</managed-property>
		<managed-property>
			<property-name>edad</property-name>
			<value>#{edad}</value>
		</managed-property>
		<managed-property>
			<property-name>telefono</property-name>
			<value>#{telefon}</value>
		</managed-property>
		-->
	</managed-bean>
</faces-config>